{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-storyshots": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.18",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.1",
    "@types/chance": "^1.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "chance": "^1.1.7",
    "date-fns": "^2.17.0",
    "material-ui-dropzone": "^3.5.0",
    "miragejs": "^0.1.41",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-testing-kit": "^2.0.2",
    "react-use-localstorage": "^3.5.3",
    "storybook-react-router": "^1.0.8",
    "superstruct": "^0.14.2",
    "swr": "^0.4.2",
    "typescript": "^4.1.5",
    "variant": "^2.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:check && npm run test:unit",
    "test:check": "prettier --check \"{.storybook,src}/**/*.{js,ts,tsx}\"",
    "test:unit": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"{.storybook,src}/**/*.{js,ts,tsx}\"",
    "storybook:start": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/testing/**"
    ],
    "moduleNameMapper": {
      "^react-use-localstorage$": "<rootDir>/node_modules/react-use-localstorage/dist/react-use-localstorage.cjs.development.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
